<p id="notice"><%= notice %></p>

<h1>Interiors</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>City</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interiors.each do |interior| %>
      <tr>
        <td><%= interior.name %></td>
        <td><%= interior.email %></td>
        <td><%= interior.mobile %></td>
        <td><%= interior.city %></td>
        <td><%= link_to 'Show', interior %></td>
        <td><%= link_to 'Edit', edit_interior_path(interior) %></td>
        <td><%= link_to 'Destroy', interior, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interior', new_interior_path %>
